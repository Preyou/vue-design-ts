{
  "name": "vue-design-ts",
  "private": true,
  "version": "0.0.0",
  "files": [
    "lib"
  ],
  "main": "./lib/vue-design-ts.umd.js",
  "module": "./lib/vue-design-ts.mjs",
  "types": "./lib/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/vue-design-ts.mjs",
      "require": "./lib/vue-design-ts.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
    "build-ts": "vue-tsc --noEmit -p tsconfig.build.json",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{js,jsx,vue,ts,tsx} --fix",
    "Type check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@icon-park/vue-next": "^1.4.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.2",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "naive-ui": "^2.30.3",
    "stylelint": "^14.9.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "typescript": "^4.5.4",
    "unplugin-auto-import": "^0.8.7",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^1.2.0",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.38.2"
  }
}